--- 
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description">
		<meta name="viewport" content="width=device-width" />
		<meta property="og:title" content="Town Of Triangle"/>
		<meta property="og:description" content="Welcome to the Town of Triangle."/>
		<meta property="og:image" content="/assets/new-tot.png"/>
		<meta property="og:url" content="https://www.townoftriangle.org/"/>
		<meta property="og:type" content="website" />

		<link rel="icon" type="image/png" href="/assets/new-tot.png" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>

